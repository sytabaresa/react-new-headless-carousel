{
    "name": "react-new-headless-carousel",
    "description": "a lightweight and simple hooks based headless api for make carousel",
    "version": "0.0.1",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "vitebook:dev": "vitebook dev",
        "vitebook:build": "vitebook build",
        "vitebook:preview": "vitebook preview",
        "lint": "run-s lint:*",
        "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx . --ignore-path .gitignore",
        "format": "run-s format:*",
        "format:eslint": "npm run lint:eslint -- --fix",
        "lint:prettier": "prettier . --check --ignore-path .gitignore --loglevel warn",
        "format:prettier": "npm run lint:prettier -- --write",
        "prepare": "husky install"
    },
    "dependencies": {
        "animated-scroll-to": "^2.3.0",
        "lodash": "^4.17.21",
        "preact": "^10.5.14",
        "preact-render-to-string": "^5.1.19",
        "react-robot": "^0.2.3",
        "robot3": "^0.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@preact/preset-vite": "^2.3.0",
        "@types/lodash": "^4.14.186",
        "@typescript-eslint/eslint-plugin": "^4.0.0",
        "@typescript-eslint/parser": "^4.0.0",
        "@vitebook/client": "^0.100.5",
        "@vitebook/core": "^0.100.5",
        "@vitebook/markdown-preact": "^0.100.5",
        "@vitebook/preact": "^0.100.5",
        "@vitebook/theme-default": "^0.100.5",
        "autoprefixer": "^10.0.0",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.0",
        "lint-staged": "^11.0.0",
        "npm-run-all": "^4.0.0",
        "postcss": "^8.0.0",
        "prettier": "^2.0.0",
        "svelte": "^3.49.0",
        "tailwindcss": "^3.0.0",
        "typescript": "^4.4.0",
        "vite": "^3.0.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
        "*.{js,jsx,ts,tsx,md,json}": "prettier --write"
    }
}
